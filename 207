        If dgvInformationRecord.RowCount > 1 Then
            For wInt_Counter1 As Integer = 0 To dgvInformationRecord.Rows.Count - 2
                dgvInformationRecord.Rows(wInt_Counter1).DefaultCellStyle.BackColor = Color.LightGreen
                wInt_Counter1 = wInt_Counter1 + 1
            Next
        End If
    End Sub

    'event click line in datagridview to show information
    Private Sub dgvInformationRecord_CellClick(sender As Object, e As DataGridViewCellEventArgs) Handles dgvInformationRecord.CellClick

        dgvInformationRecord.SelectionMode = DataGridViewSelectionMode.FullRowSelect
        rtbReceiptDetail.Clear()
        If dgvInformationRecord.RowCount > 1 Then
            If e.RowIndex < dgvInformationRecord.RowCount - 1 Then

