    End Sub

    'get information from richtextbox to print 
    Private Sub pdtPrintDocument_PrintPage(sender As Object, e As Printing.PrintPageEventArgs) Handles pdtPrintDocument.PrintPage

        Static wInt_CurrentChar As Integer
        Static wInt_CurrentLine As Integer
        Dim wFnt_TextFont As Font = rtbReceiptDetail.Font
        Dim wInt_Height As Integer
        Dim wInt_Witdh As Integer
        Dim wInt_Left As Integer
        Dim wInt_Top As Integer
        With pdtPrintDocument.DefaultPageSettings
            wInt_Height = .PaperSize.Height - .Margins.Top - .Margins.Bottom
            wInt_Witdh = .PaperSize.Width - .Margins.Left - .Margins.Right
            wInt_Left = pdtPrintDocument.DefaultPageSettings.Margins.Left
            wInt_Top = pdtPrintDocument.DefaultPageSettings.Margins.Top
        End With
        'Optional Rectangle Blue.
        'e.Graphics.DrawRectangle(Pens.Blue, New Rectangle(left, top, w, h))
        If pdtPrintDocument.DefaultPageSettings.Landscape Then
            Dim wInt_temp As Integer
            wInt_temp = wInt_Height
            wInt_Height = wInt_Witdh
            wInt_Witdh = wInt_temp
        End If
        Dim wInt_Lines As Integer
        wInt_Lines = CInt(Math.Round(wInt_Height / wFnt_TextFont.Height))
        Dim wRec_Rectangle As New Rectangle(wInt_Left, wInt_Top, wInt_Witdh, wInt_Height)
        Dim wFmt_format As StringFormat
        If Not rtbReceiptDetail.WordWrap Then
            wFmt_format = New StringFormat(StringFormatFlags.NoWrap)
            wFmt_format.Trimming = StringTrimming.EllipsisWord
            Dim wInt_Counter1 As Integer
            For wInt_Counter1 = wInt_CurrentLine To Math.Min(wInt_CurrentLine + wInt_Lines, rtbReceiptDetail.Lines.Length - 1)
                e.Graphics.DrawString(rtbReceiptDetail.Lines(wInt_Counter1), wFnt_TextFont, Brushes.Black, New RectangleF(wInt_Left, wInt_Top + wFnt_TextFont.Height * (wInt_Counter1 - wInt_CurrentLine), wInt_Witdh, wFnt_TextFont.Height), wFmt_format)
            Next wInt_Counter1
            wInt_CurrentLine += wInt_Lines
            If wInt_CurrentLine >= txtShopCode.Lines.Length Then
                e.HasMorePages = False
                wInt_CurrentLine = 0
            Else
                e.HasMorePages = True
            End If
            Exit Sub
        End If
        wFmt_format = New StringFormat(StringFormatFlags.LineLimit)
        Dim wInt_Line As Integer
        Dim wInt_Chars As Integer
        e.Graphics.MeasureString(Mid(rtbReceiptDetail.Text, wInt_CurrentChar + 1), wFnt_TextFont, New SizeF(wInt_Witdh, wInt_Height), wFmt_format, wInt_Chars, wInt_Line)
        If wInt_CurrentChar + wInt_Chars < rtbReceiptDetail.Text.Length Then
            If rtbReceiptDetail.Text.Substring(wInt_CurrentChar + wInt_Chars, 1) <> " " And rtbReceiptDetail.Text.Substring(wInt_CurrentChar + wInt_Chars, 1) <> vbLf Then
                While wInt_Chars > 0
                    rtbReceiptDetail.Text.Substring(wInt_CurrentChar + wInt_Chars, 1)
                    rtbReceiptDetail.Text.Substring(wInt_CurrentChar + wInt_Chars, 1)
                    wInt_Chars -= 1
                End While
                wInt_Chars += 1
            End If
        End If
        e.Graphics.DrawString(rtbReceiptDetail.Text.Substring(wInt_CurrentChar, wInt_Chars), wFnt_TextFont, Brushes.Black, wRec_Rectangle, wFmt_format)
        wInt_CurrentChar = wInt_CurrentChar + wInt_Chars
        If wInt_CurrentChar < rtbReceiptDetail.Text.Length Then
            e.HasMorePages = True
        Else
            e.HasMorePages = False
            wInt_CurrentChar = 0
        End If
    End Sub
